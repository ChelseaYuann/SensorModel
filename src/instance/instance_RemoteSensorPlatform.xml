<?xml version="1.0" encoding="UTF-8" standalone="no"?><sml:PhysicalComponent xmlns:sml="http://www.opengis.net/sensorml/2.0" xmlns:gco="http://www.isotc211.org/2005/gco" xmlns:gmd="http://www.isotc211.org/2005/gmd" xmlns:gml="http://www.opengis.net/gml/3.2" xmlns:swe="http://www.opengis.net/swe/2.0" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" id="RemoteSensorPlatform" xsi:schemaLocation="http://www.opengis.net/sensorml/2.0 http://schemas.opengis.net/sensorML/2.0/sensorML.xsd">
	<!-- ================================================= -->
	<!--         RemoteSensorPlatform ==>Description      -->
	<!-- ================================================= -->
	<gml:description>卫星平台</gml:description>
	<gml:name>卫星传感器平台</gml:name>
	<!-- ================================================= -->
	<!--                    keywords                       -->
	<!-- ================================================= -->
	<sml:keywords>
		<sml:KeywordList>
			<sml:keyword/>
			<sml:keyword/>
			<sml:keyword/>
			<sml:keyword/>
		</sml:KeywordList>
	</sml:keywords>
	<!-- ================================================= -->
	<!--                   identification                  -->
	<!-- ================================================= -->
	<sml:identification>
		<sml:IdentifierList>
			<sml:identifier>
				<sml:Term definition="urn:ogc:def:identifier:OGC:1.0:uniqueID">
					<sml:label>唯一标识符</sml:label>
					<sml:value>urn:CUG:def:identifier:OGC:2.0:swz</sml:value>
				</sml:Term>
			</sml:identifier>
			<sml:identifier>
				<sml:Term definition="urn:ogc:def:identifier:OGC:1.0:longName">
					<sml:label>卫星平台全称</sml:label>
					<sml:value>platform</sml:value>
				</sml:Term>
			</sml:identifier>
			<sml:identifier>
				<sml:Term definition="urn:ogc:def:identifier:OGC:1.0:shortName">
					<sml:label>卫星平台简称</sml:label>
					<sml:value>plat</sml:value>
				</sml:Term>
			</sml:identifier>
			<sml:identifier>
				<sml:Term definition="urn:ogc:def:identifier:OGC:1.0:associatedSensorName">
					<sml:label>搭载传感器名称</sml:label>
					<sml:value>【*】平台简称</sml:value>
				</sml:Term>
			</sml:identifier>
			<sml:identifier>
				<sml:Term definition="urn:ogc:def:identifier:OGC:1.0:associatedSensorUniqueID">
					<sml:label>搭载传感器标识符</sml:label>
					<sml:value>【*】urn:ogc:feature:remoteSensor:platform::XXX</sml:value>
				</sml:Term>
			</sml:identifier>
		</sml:IdentifierList>
	</sml:identification>
	<!-- ================================================= -->
	<!--                   classification                  -->
	<!-- ================================================= -->
	<sml:classification>
		<sml:ClassifierList>
			<sml:classifier>
				<sml:Term definition="urn:ogc:def:classifier:OGC:1.0:platformType">
					<sml:label>平台类型</sml:label>
					<sml:value/>
				</sml:Term>
			</sml:classifier>
			<sml:classifier>
				<sml:Term definition="urn:ogc:def:classifier:OGC:1.0:sensorType">
					<sml:label>卫星类型</sml:label>
					<sml:value>陆地卫星/气象卫星/海洋卫星/导航卫星/多用途卫星/其他卫星（任选一种）</sml:value>
				</sml:Term>
			</sml:classifier>
			<sml:classifier>
				<sml:Term definition="urn:ogc:def:classifier:OGC:1.0:orbitType">
					<sml:label>轨道类型</sml:label>
					<sml:value>太阳同步轨道/地球同步轨道（任选一种）</sml:value>
				</sml:Term>
			</sml:classifier>
			<sml:classifier>
				<sml:Term definition="urn:ogc:def:classifier:OGC:1.0:intendedApplication">
					<sml:label>预期应用</sml:label>
					<sml:value>对地观测成像/洪水观测/……（此结构可有多个）</sml:value>
				</sml:Term>
			</sml:classifier>
		</sml:ClassifierList>
	</sml:classification>
	<!-- ================================================= -->
	<!--                     constraints                    -->
	<!-- ================================================= -->
	<!--     constraints==>validTime   -->
	<sml:validTime>
		<gml:TimePeriod id="卫星平台有效时间">
			<gml:beginPosition/>
			<gml:endPosition/>
		</gml:TimePeriod>
	</sml:validTime>
	<!--     constraints==>securityConstraint   -->
	
	<!--     constraints==>legalConstraint   -->
	
	<!-- ================================================= -->
	<!--                    characteristics                -->
	<!-- ================================================= -->
	<sml:characteristics name="generalProperties">
		<sml:CharacteristicList>
			
			
			
		</sml:CharacteristicList>
	</sml:characteristics>
	<!-- ================================================= -->
	<!--                     capabilities                  -->
	<!-- ================================================= -->
	<sml:capabilities>
		<sml:CapabilityList>
			
			
			
			
		</sml:CapabilityList>
	</sml:capabilities>
	<!-- ================================================= -->
	<!--                      Location                     -->
	<!-- ================================================= -->
	<sml:position>
		<swe:Vector definition="urn:ogc:def:property:OGC:1.0:remoteSensorPlatformLocation" referenceFrame="urn:ogc:def:crs:OGC:1.3:EPSG:4329">
			<swe:coordinate name="纬度">
				<swe:Quantity axisID="y" definition="urn:ogc:def:property:OGC:1.0:Latitude">
					<swe:uom code="deg"/>
					<swe:value/>
				</swe:Quantity>
			</swe:coordinate>
			<swe:coordinate name="经度">
				<swe:Quantity axisID="x" definition="urn:ogc:def:property:OGC:1.0:Longitude">
					<swe:uom code="deg"/>
					<swe:value/>
				</swe:Quantity>
			</swe:coordinate>
			<swe:coordinate name="海拔">
				<swe:Quantity axisID="z" definition="urn:ogc:def:property:OGC:1.0:Altitude">
					<swe:uom code="deg"/>
					<swe:value/>
				</swe:Quantity>
			</swe:coordinate>
		</swe:Vector>
	</sml:position>
	<sml:SpatialFrame id="">
		<sml:origin/>
		<sml:axis name="X"/>
		<sml:axis name="Y"/>
		<sml:axis name="Z"/>
	</sml:SpatialFrame>
	<sml:TemporalFrame id="">
		<sml:origin/>
	</sml:TemporalFrame>
	<!-- ================================================= -->
	<!--               responsible department              -->
	<!-- ================================================= -->
	<sml:contacts>
		<sml:ContactList>
			<!--     operator     -->
			<sml:contact xlink:arcrole="urn:ogc:def:role:OGC:1.0:manufacturer">
				<gmd:CI_ResponsibleParty>
					<gmd:organisationName>
						<gco:CharacterString/>
					</gmd:organisationName>
					<gmd:individualName>
						<gco:CharacterString/>
					</gmd:individualName>
					<!--  contactInfo  -->
					<gmd:contactInfo>
						<gmd:CI_Contact>
							<gmd:phone>
								<gmd:CI_Telephone>
									<gmd:voice/>
								</gmd:CI_Telephone>
							</gmd:phone>
							<gmd:address>
								<gmd:CI_Address>
									<gmd:deliveryPoint/>
									<gmd:city/>
									<gmd:administrativeArea/>
									<gmd:country/>
									<gmd:electronicMailAddress/>
									<gmd:postalCode/>
								</gmd:CI_Address>
							</gmd:address>
						</gmd:CI_Contact>
					</gmd:contactInfo>
				</gmd:CI_ResponsibleParty>
			</sml:contact>
		</sml:ContactList>
	</sml:contacts>
	<!-- ================================================= -->
	<!--       external resource for documentation         -->
	<!-- ================================================= -->
	<sml:documentation>
		<sml:DocumentList>
			<sml:document xlink:arcrole="urn:ogc:def:property:OGC:1.0:specification(示例)">
				<gmd:CI_OnlineResource>
					<gmd:linkage>
						<gmd:URL>null(附件URL)</gmd:URL>
					</gmd:linkage>
					<gmd:name>
						<gco:CharacterString>null(附件名称)</gco:CharacterString>
					</gmd:name>
					<gmd:description>
						<gco:CharacterString>null(附件简单描述)</gco:CharacterString>
					</gmd:description>
				</gmd:CI_OnlineResource>
			</sml:document>
		</sml:DocumentList>
	</sml:documentation>
	<!-- ================================================= -->
	<!--                     History                       -->
	<!-- ================================================= -->
	
