						</gmd:CI_Contact>
					</gmd:contactInfo>
				</gmd:CI_ResponsibleParty>
			</sml:contact>
		</sml:ContactList>
	</sml:contacts>
	<!-- ================================================= -->
	<!--       external resource for documentation         -->
