					</gmd:organisationName>
					<gmd:individualName>
						<gco:CharacterString>null</gco:CharacterString>
					</gmd:individualName>
					<!--  contactInfo  -->
					<gmd:contactInfo>
						<gmd:CI_Contact>
							<gmd:phone>
								<gmd:CI_Telephone>
									<gmd:voice>null</gmd:voice>
