<?xml version="1.0" encoding="UTF-8"?>
<sml:PhysicalComponent id="FrameSensor" xmlns:sml="http://www.opengis.net/sensorml/2.0" xmlns:swe="http://www.opengis.net/swe/2.0" xmlns:gml="http://www.opengis.net/gml/3.2" xmlns:gmd="http://www.isotc211.org/2005/gmd" xmlns:gco="http://www.isotc211.org/2005/gco" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:ism="urn:us:gov:ic:ism" xsi:schemaLocation="http://www.opengis.net/sensorml/2.0  http://schemas.opengis.net/sensorML/2.0/sensorML.xsd">
	<!-- ================================================= -->
	<!--             FrameSensor ==>Description           -->
	<!-- ================================================= -->
	<gml:description>摄影类型卫星传感器概述</gml:description>
	<gml:name>摄影成像传感器</gml:name>
	<!-- ================================================= -->
	<!--                    keywords                                             -->
	<!-- ================================================= -->
	<sml:keywords>
		<sml:KeywordList>
			<sml:keyword>传感器中文简称</sml:keyword>
			<sml:keyword>传感器英文简称</sml:keyword>
			<sml:keyword>卫星平台简称</sml:keyword>
			<sml:keyword>典型应用</sml:keyword>
		</sml:KeywordList>
	</sml:keywords>
	<!-- ================================================= -->
	<!--                   identification                     -->
	<!-- ================================================= -->
	<sml:identification>
		<sml:IdentifierList>
			<sml:identifier>
				<sml:Term definition="urn:ogc:def:identifier:OGC:1.0:uniqueID">
					<sml:label>唯一标识码</sml:label>
					<sml:value>【*】urn:组织标识英文名:def:identifier:OGC:2.0:传感器英文简称(不能出现空格)</sml:value>
				</sml:Term>
			</sml:identifier>
			<sml:identifier>
				<sml:Term definition="urn:ogc:def:identifier:OGC:1.0:longName">
					<sml:label>传感器全称</sml:label>
					<sml:value>【*】全称</sml:value>
				</sml:Term>
			</sml:identifier>
			<sml:identifier>
				<sml:Term definition="urn:ogc:def:identifier:OGC:1.0:shortName">
					<sml:label>传感器简称</sml:label>
					<sml:value>【*】简称</sml:value>
				</sml:Term>
			</sml:identifier>
			<sml:identifier>
				<sml:Term definition="urn:ogc:def:identifier:OGC:1.0:parentSensorPlatformName">
					<sml:label>所属平台名称</sml:label>
					<sml:value>【*】平台简称</sml:value>
				</sml:Term>
			</sml:identifier>
			<sml:identifier>
				<sml:Term definition="urn:ogc:def:identifier:OGC:1.0:parentSensorPlatformUniqueID">
					<sml:label>所属平台标识符</sml:label>
					<sml:value>【*】urn:ogc:feature:remoteSensor:platform::XXX</sml:value>
				</sml:Term>
			</sml:identifier>
		</sml:IdentifierList>
	</sml:identification>
	<!-- ================================================= -->
	<!--                   classification                    -->
	<!-- ================================================= -->
	<sml:classification>
		<sml:ClassifierList>
			<sml:classifier>
				<sml:Term definition="urn:ogc:def:classifier:OGC:1.0:sensorType">
					<sml:label>传感器类型</sml:label>
					<sml:value>摄影类型传感器</sml:value>
				</sml:Term>
			</sml:classifier>
			<sml:classifier>
				<sml:Term definition="urn:ogc:def:classifier:OGC:1.0:frameType">
					<sml:label>摄影类型</sml:label>
					<sml:value>【*】框幅式摄影机/缝隙式摄影机/多光谱摄影机/全景摄影机(任选一种)</sml:value>
				</sml:Term>
			</sml:classifier>
			<sml:identifier>
				<sml:Term definition="urn:ogc:def:classifier:OGC:1.0:intendedApplication">
					<sml:label>预期应用</sml:label>
					<sml:value>【*】对地观测成像</sml:value>
				</sml:Term>
			</sml:identifier>
		</sml:ClassifierList>
	</sml:classification>
	<!-- ================================================= -->
	<!--                     constraints                    -->
	<!-- ================================================= -->
	<!--     constraints==>validTime   -->
	<sml:validTime>
		<gml:TimePeriod id="传感器有效时间">
			<gml:beginPosition>2016-01-01T08:00:00.0Z</gml:beginPosition>
			<gml:endPosition>2026-01-01T08:00:00.0Z</gml:endPosition>
		</gml:TimePeriod>
	</sml:validTime>
	<!--     constraints==>securityConstraint   -->
	<sml:securityConstraints>
		<gmd:MD_SecurityConstraints>
			<gmd:useLimitation>
				<gco:CharacterString>安全约束简单描述</gco:CharacterString>
			</gmd:useLimitation>
		</gmd:MD_SecurityConstraints>
	</sml:securityConstraints>
	<!--     constraints==>legalConstraint   -->
	<sml:legalConstraints>
		<gmd:MD_LegalConstraints>
			<gmd:useLimitation>
				<gco:CharacterString>法律约束简单描述</gco:CharacterString>
			</gmd:useLimitation>
		</gmd:MD_LegalConstraints>
	</sml:legalConstraints>
	<!-- ================================================= -->
	<!--                    characteristics                  -->
	<!-- ================================================= -->
	<sml:characteristics name="generalProperties">
		<sml:CharacteristicList>
			<sml:characteristic name="physicalProperties">
				<swe:DataRecord definition="urn:ogc:def:property:OGC:1.0:PhysicalProperties">
					<swe:label>传感器物理特征</swe:label>
					<swe:field name="长度">
						<swe:Quantity definition="urn:ogc:def:property:OGC:1.0:length">
							<swe:uom>m</swe:uom>
							<swe:value>00</swe:value>
						</swe:Quantity>
					</swe:field>
					<swe:field name="宽度">
						<swe:Quantity definition="urn:ogc:def:property:OGC:1.0:width">
							<swe:uom>m</swe:uom>
							<swe:value>00</swe:value>
						</swe:Quantity>
					</swe:field>
					<swe:field name="高度">
						<swe:Quantity definition="urn:ogc:def:property:OGC:1.0:height">
							<swe:uom>m</swe:uom>
							<swe:value>00</swe:value>
						</swe:Quantity>
					</swe:field>
					<swe:field name="质量">
						<swe:Quantity definition="urn:ogc:def:property:OGC:1.0:weight">
							<swe:uom>kg</swe:uom>
							<swe:value>00</swe:value>
						</swe:Quantity>
					</swe:field>
				</swe:DataRecord>
			</sml:characteristic>
			<sml:characteristic name="electricalRequirements">
				<swe:DataRecord definition="urn:ogc:def:property:OGC:1.0:powerRequirement">
					<swe:label>供电通信特征</swe:label>
					<swe:field name="电流">
						<swe:QuantityRange definition="urn:ogc:def:property:OGC:1.0:electricalCurrent">
							<swe:uom code="A"/>
							<swe:value>00 00</swe:value>
						</swe:QuantityRange>
					</swe:field>
					<swe:field name="电压">
						<swe:QuantityRange definition="urn:ogc:def:property:OGC:1.0:voltage">
							<swe:uom code="V"/>
							<swe:value>00 00</swe:value>
						</swe:QuantityRange>
					</swe:field>
					<swe:field name="功率">
						<swe:Quantity definition="urn:ogc:def:property:OGC:1.0:power">
							<swe:uom code="W"/>
							<swe:value>00</swe:value>
						</swe:Quantity>
					</swe:field>
					<swe:field name="输出功率">
						<swe:Quantity definition="urn:ogc:def:property:OGC:1.0:outputPower">
							<swe:uom code="W"/>
							<swe:value>00</swe:value>
						</swe:Quantity>
					</swe:field>
				</swe:DataRecord>
			</sml:characteristic>
			<sml:characteristic name="workEnvironment">
				<swe:DataRecord definition="urn:ogc:def:property:OGC:1.0:workEnvironment">
					<swe:label>工作环境</swe:label>
					<swe:field name="设计寿命">
						<swe:Quantity definition="urn:ogc:def:property:OGC:1.0:designedLife">
							<swe:uom code="year"/>
							<swe:value>00</swe:value>
						</swe:Quantity>
					</swe:field>
				</swe:DataRecord>
			</sml:characteristic>
		</sml:CharacteristicList>
	</sml:characteristics>
	<!-- ================================================= -->
	<!--                     capabilities                      -->
	<!-- ================================================= -->
	<sml:capabilities name="specifications">
		<sml:CapabilityList>
			<!--      capabilities==>observationBreath        -->
			<sml:capability name="FrameRSObservationBreadth">
				<swe:DataRecord definition="urn:ogc:def:property:OGC:1.0:FrameRSObservationBreadth">
					<swe:label>观测能力广度</swe:label>
					<swe:field name="observedBBox">
						<gml:Envelope definition="urn:ogc:def:property:OGC:1.0:observedBBOX" referenceFrame="urn:ogc:def:crs:OGC:1.3:EPSG:4329">
							<gml:lowerCorner>
								<swe:Vector>
									<swe:coordinate name="纬度">
										<swe:Quantity axisID="y">
											<swe:uom code="deg" xlink:href="urn:ogc:def:uom:OGC:1.0:UCUM::deg"/>
											<swe:value>00</swe:value>
										</swe:Quantity>
									</swe:coordinate>
									<swe:coordinate name="经度">
										<swe:Quantity axisID="x">
											<swe:uom code="deg" xlink:href="urn:ogc:def:uom:OGC:1.0:UCUM::deg"/>
											<swe:value>00</swe:value>
										</swe:Quantity>
									</swe:coordinate>
								</swe:Vector>
							</gml:lowerCorner>
							<gml:upperCorner>
								<swe:Vector>
									<swe:coordinate name="纬度">
										<swe:Quantity axisID="y">
											<swe:uom code="deg" xlink:href="urn:ogc:def:uom:OGC:1.0:UCUM::deg"/>
											<swe:value>00</swe:value>
										</swe:Quantity>
									</swe:coordinate>
									<swe:coordinate name="经度">
										<swe:Quantity axisID="x">
											<swe:uom code="deg" xlink:href="urn:ogc:def:uom:OGC:1.0:UCUM::deg"/>
											<swe:value>00</swe:value>
										</swe:Quantity>
									</swe:coordinate>
								</swe:Vector>
							</gml:upperCorner>
						</gml:Envelope>
					</swe:field>
					<swe:field name="波段类别">
						<swe:Category definition="urn:ogc:def:property:OGC:1.0:bandsCategory">
							<swe:value>【*】VIS/NIR/SWIR/MWIR/TIR/FIR/ MW/K-Band/X-Band/C-Band/S-Band/L-Band /P-Band</swe:value>
						</swe:Category>
					</swe:field>
					<swe:field name="波段范围">
						<swe:QuantityRange definition="urn:ogc:def:property:OGC:1.0:bandsRange">
							<swe:uom code="um"/>
							<swe:value>00 00</swe:value>
						</swe:QuantityRange>
					</swe:field>
					<swe:field name="波段数">
						<swe:Count definition="urn:ogc:def:property:OGC:1.0:bandsNumber">
							<swe:value>00</swe:value>
						</swe:Count>
					</swe:field>
					<swe:field name="成像宽度范围">
						<swe:QuantityRange definition="urn:ogc:def:property:OGC:1.0:Swath">
							<swe:uom code="km"/>
							<sml:value>00 00</sml:value>
						</swe:QuantityRange>
					</swe:field>
					<swe:field name="视场角">
						<swe:Quantity definition="urn:ogc:def:property:OGC:1.0:FOV">
							<swe:uom code="deg"/>
							<swe:value>00</swe:value>
						</swe:Quantity>
					</swe:field>
					<swe:field name="地面分辨率范围">
						<swe:QuantityRange definition="urn:ogc:def:property:OGC:1.0:groundResolutionRange">
							<swe:uom code="m"/>
							<swe:value>00 00</swe:value>
						</swe:QuantityRange>
					</swe:field>
					<swe:field name="焦距">
						<swe:Quantity definition="urn:ogc:def:property:OGC:1.0:focalLength">
							<swe:uom code="m"/>
							<swe:value>00</swe:value>
						</swe:Quantity>
					</swe:field>
					<swe:field name="摄影比例尺">
						<swe:Quantity definition="urn:ogc:def:property:OGC:1.0:imageScale">
							<swe:uom code="m"/>
							<swe:value>00</swe:value>
						</swe:Quantity>
					</swe:field>
				</swe:DataRecord>
			</sml:capability>
			<!--      capabilities==>observationDepth        -->
			<sml:capability name="FrameRSObservationDepth">
				<swe:DataRecord definition="urn:ogc:def:property:OGC:1.0:FrameRSObservationDepth">
					<swe:label>观测能力深度</swe:label>
					<!--   optical_BandCharacteristics   -->
					<swe:field name="观测波段特征">
						<swe:DataRecord>
							<swe:field name="RED波段名称(示例)">
								<swe:DataRecord>
									<swe:field name="波段宽度范围">
										<swe:QuantityRange definition="urn:ogc:def:property:OGC:1.0:bandWidthRange">
											<swe:uom code="um"/>
											<swe:value>00 00</swe:value>
										</swe:QuantityRange>
									</swe:field>
									<swe:field name="光谱分辨率">
										<swe:Quantity definition="urn:ogc:def:property:OGC:1.0:spectralResolution">
											<swe:uom code="nm"/>
											<swe:value>00</swe:value>
										</swe:Quantity>
									</swe:field>
									<swe:field name="辐射分辨率">
										<swe:Quantity definition="urn:ogc:def:property:OGC:1.0:radiationResolution">
											<swe:value>00</swe:value>
										</swe:Quantity>
									</swe:field>
									<swe:field name="波段应用">
										<swe:Text definition="urn:ogc:def:property:OGC:1.0:bandAssociatedApplication">
											<swe:value>植被,土壤</swe:value>
										</swe:Text>
									</swe:field>
								</swe:DataRecord>
							</swe:field>
						</swe:DataRecord>
					</swe:field>
					<swe:field name="灰阶">
						<swe:Count definiton="urn:ogc:def:property:OGC:1.0:greyLevel">
							<swe:value>00</swe:value>
						</swe:Count>
					</swe:field>
					<swe:field name="量化等级">
						<swe:Quantity definiton="urn:ogc:def:property:OGC:1.0:quantizationLevel">
							<swe:value>00</swe:value>
						</swe:Quantity>
					</swe:field>
					<swe:field name="解像率">
						<swe:Quantity definiton="urn:ogc:def:property:OGC:1.0:resolvingPower">
							<swe:uom code="%"/>
							<swe:value>00</swe:value>
						</swe:Quantity>
					</swe:field>
					<swe:field name="航向重叠率">
						<swe:Quantity definiton="urn:ogc:def:property:OGC:1.0:forwardOverlap">
							<swe:uom code="%"/>
							<swe:value>00</swe:value>
						</swe:Quantity>
					</swe:field>
					<swe:field name="旁向重叠率">
						<swe:Quantity definiton="urn:ogc:def:property:OGC:1.0:lateralOverlap">
							<swe:uom code="%"/>
							<swe:value>00</swe:value>
						</swe:Quantity>
					</swe:field>
				</swe:DataRecord>
			</sml:capability>
			<!--      capabilities==>observationFrequency        -->
			<sml:capability name="FrameRSObservationFrequency">
				<swe:DataRecord definition="urn:ogc:def:property:OGC:1.0:FrameRSObservationFrequency">
					<swe:label>观测能力频度</swe:label>
					<swe:field name="重访周期">
						<swe:Quantity definition="urn:ogc:def:property:revisitTime">
							<swe:uom code="day"/>
							<swe:value>00</swe:value>
						</swe:Quantity>
					</swe:field>
					<swe:field name="帧速度">
						<swe:Quantity definition="urn:ogc:def:property:frameRate">
							<swe:uom code="secd/f"/>
							<swe:value>00</swe:value>
						</swe:Quantity>
					</swe:field>
					<swe:field name="快门速度">
						<swe:Quantity definition="urn:ogc:def:property:shutterSpeed">
							<swe:uom code="secd"/>
							<swe:value>00</swe:value>
						</swe:Quantity>
					</swe:field>
				</swe:DataRecord>
			</sml:capability>
			<!--      capabilities==>observationQuality        -->
			<sml:capability name="FrameRSObservationQuality">
				<swe:DataRecord definition="urn:ogc:def:property:OGC:1.0:FrameRSObservationQuality">
					<swe:label>观测能力质量</swe:label>
					<swe:field name="地理定位精度">
						<swe:Quantity definition="urn:ogc:def:property:OGC:1.0:geolocationAccuracy">
							<swe:uom code="m"/>
							<swe:value>00</swe:value>
						</swe:Quantity>
					</swe:field>
					<swe:field name="角度精度">
						<swe:Quantity definition="urn:ogc:def:property:OGC:1.0:angleAccuracy">
							<swe:uom code="deg"/>
							<swe:value>00</swe:value>
						</swe:Quantity>
					</swe:field>
					<swe:field name="距离精度">
						<swe:Quantity definition="urn:ogc:def:property:OGC:1.0:distanceAccuracy">
							<swe:uom code="m"/>
							<swe:value>00</swe:value>
						</swe:Quantity>
					</swe:field>
					<swe:field name="辐射精度">
						<swe:Quantity definition="urn:ogc:def:property:OGC:1.0:radiationAccuracy">
							<swe:value>null</swe:value>
						</swe:Quantity>
					</swe:field>
				</swe:DataRecord>
			</sml:capability>
			<!--      capabilities==>observationData        -->
			<sml:capability name="FrameRSObservationData">
				<swe:DataRecord definition="urn:ogc:def:property:OGC:1.0:FrameRSObservationData">
					<swe:label>观测能力数据</swe:label>
					<swe:field name="数据获取等级">
						<swe:Category definition="urn:ogc:def:property:OGC:1.0:dataAccessLevel">
							<swe:value>公开/限制(任选一种)</swe:value>
						</swe:Category>
					</swe:field>
					<swe:field name="数据格式">
						<swe:Category definition="urn:ogc:def:property:OGC:1.0:dataFormat">
							<swe:value>null</swe:value>
						</swe:Category>
					</swe:field>
					<swe:field name="数据质量等级">
						<swe:Category definition="urn:ogc:def:property:OGC:1.0:dataQualityLevel">
							<swe:value>null</swe:value>
						</swe:Category>
					</swe:field>
				</swe:DataRecord>
			</sml:capability>
		</sml:CapabilityList>
	</sml:capabilities>
	<!-- ================================================= -->
	<!--                      Location                     -->
	<!-- ================================================= -->
	<!--    LocationByDescription    -->
	<sml:position>
		<swe:Text>
			<swe:value>传感器相对位置简单描述</swe:value>
		</swe:Text>
	</sml:position>
	<!--    LocationByPoint    -->
	<sml:position>
		<!-- EPSG 4329 is for latitude-longitude, in that order -->
		<gml:Point gml:id="传感器坐标" srsName="urn:ogc:def:crs:OGC:1.3:EPSG:4329">
			<gml:coordinates>【*】00.00 00.00</gml:coordinates>
		</gml:Point>
	</sml:position>
	<!--    LocationByLocation    -->
	<sml:position>
		<swe:Vector definition="urn:ogc:def:property:OGC:1.0:SensorLocation" referenceFrame="urn:ogc:def:crs:OGC:1.3:EPSG:4329">
			<swe:coordinate name="纬度">
				<swe:Quantity definition="urn:ogc:def:property:OGC:1.0:Latitude" axisID="y">
					<swe:uom code="deg"/>
					<swe:value>00.00</swe:value>
				</swe:Quantity>
			</swe:coordinate>
			<swe:coordinate name="经度">
				<swe:Quantity definition="urn:ogc:def:property:OGC:1.0:Longitude" axisID="x">
					<swe:uom code="deg"/>
					<swe:value>00.00</swe:value>
				</swe:Quantity>
			</swe:coordinate>
			<swe:coordinate name="海拔">
				<swe:Quantity definition="urn:ogc:def:property:OGC:1.0:Altitude" axisID="z">
					<swe:uom code="deg"/>
					<swe:value>00.00</swe:value>
				</swe:Quantity>
			</swe:coordinate>
		</swe:Vector>
	</sml:position>
