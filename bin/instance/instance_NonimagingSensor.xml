<?xml version="1.0" encoding="UTF-8" standalone="no"?><sml:PhysicalComponent xmlns:sml="http://www.opengis.net/sensorml/2.0" xmlns:gco="http://www.isotc211.org/2005/gco" xmlns:gmd="http://www.isotc211.org/2005/gmd" xmlns:gml="http://www.opengis.net/gml/3.2" xmlns:swe="http://www.opengis.net/swe/2.0" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" id="nonimagingSensor" xsi:schemaLocation="http://www.opengis.net/sensorml/2.0 http://schemas.opengis.net/sensorML/2.0/sensorML.xsd">
	<!-- ================================================= -->
	<!--             NonimagingSensor ==>Description            -->
	<!-- ================================================= -->
	<gml:description/>
	<gml:name>非图像类型传感器</gml:name>
	<!-- ================================================= -->
	<!--                    keywords                       -->
	<!-- ================================================= -->
	<sml:keywords>
		<sml:KeywordList>
			<sml:keyword/>
			<sml:keyword/>
			<sml:keyword/>
			<sml:keyword/>
		</sml:KeywordList>
	</sml:keywords>
	<!-- ================================================= -->
	<!--                   identification                  -->
	<!-- ================================================= -->
	<sml:identification>
		<sml:IdentifierList>
			<sml:identifier>
				<sml:Term definition="urn:ogc:def:identifier:OGC:1.0:uniqueID">
					<sml:label>唯一标识码</sml:label>
					<sml:value/>
				</sml:Term>
			</sml:identifier>
			<sml:identifier>
				<sml:Term definition="urn:ogc:def:identifier:OGC:1.0:longName">
					<sml:label>传感器全称</sml:label>
					<sml:value/>
				</sml:Term>
			</sml:identifier>
			<sml:identifier>
				<sml:Term definition="urn:ogc:def:identifier:OGC:1.0:shortName">
					<sml:label>传感器简称</sml:label>
					<sml:value/>
				</sml:Term>
			</sml:identifier>
			<sml:identifier>
				<sml:Term definition="urn:ogc:def:identifier:OGC:1.0:parentSensorPlatformName">
					<sml:label>所属平台名称</sml:label>
					<sml:value>【*】平台简称</sml:value>
				</sml:Term>
			</sml:identifier>
			<sml:identifier>
				<sml:Term definition="urn:ogc:def:identifier:OGC:1.0:parentSensorPlatformUniqueID">
					<sml:label>所属平台标识符</sml:label>
					<sml:value>【*】urn:ogc:feature:insiteSensor:platform::XXX</sml:value>
				</sml:Term>
			</sml:identifier>
		</sml:IdentifierList>
	</sml:identification>
	<!-- ================================================= -->
	<!--                      Classification               -->
	<!-- ================================================= -->
	<sml:classification>
		<sml:ClassifierList>
			<sml:classifier>
				<sml:Term definition="urn:ogc:def:classifier:OGC:1.0:sensorType">
					<sml:label>传感器类型</sml:label>
					<sml:value/>
				</sml:Term>
			</sml:classifier>
			<sml:classifier>
				<sml:Term definition="urn:ogc:def:classifier:OGC:1.0:sensorType">
					<sml:label>传感器类型</sml:label>
					<sml:value>【*】微波散射计/微波高度计/激光光谱仪/激光高度计/激光水深计/激光测距仪/其他(任填一类)</sml:value>
				</sml:Term>
			</sml:classifier>
			<sml:classifier>
				<sml:Term definition="urn:ogc:def:classifier:OGC:1.0:sharingLevel">
					<sml:label>共享等级</sml:label>
					<sml:value>【*】公开/限制(任选一类)</sml:value>
				</sml:Term>
			</sml:classifier>
			<sml:classifier>
				<sml:Term definition="urn:ogc:def:classifier:OGC:1.0:intendedApplication">
					<sml:label>预期应用</sml:label>
					<sml:value>【*】海洋监测/气象预报等等(可填1~n项)</sml:value>
				</sml:Term>
			</sml:classifier>
		</sml:ClassifierList>
	</sml:classification>
	<!-- ================================================= -->
	<!--                      Constraints                  -->
	<!-- ================================================= -->
	<sml:validTime>
		<gml:TimePeriod gml:id="传感器有效时间">
			<gml:beginPosition/>
			<gml:endPosition/>
		</gml:TimePeriod>
	</sml:validTime>
	
	
	<!-- ================================================= -->
	<!--                     Characteristics               -->
	<!-- ================================================= -->
	<sml:characteristics name="generalProperties">
		<sml:CharacteristicList>
			
			
			
		</sml:CharacteristicList>
	</sml:characteristics>
	<!-- ================================================= -->
	<!--                    Capability                     -->
	<!-- ================================================= -->
	<sml:capabilities name="specifications">
		<sml:CapabilityList>
			<!-- Observation Breadth -->
			
			<!-- Observation Depth -->
			
			<!-- Observation Frequency -->
			
			<!-- Observation Quality -->
			
			<!-- Observation Data -->
			
		</sml:CapabilityList>
	</sml:capabilities>
	<!-- ================================================= -->
	<!--                      Location                     -->
	<!-- ================================================= -->
	
	<!--    LocationByPoint    -->
	
	<!--    LocationByLocation    -->
	
	<!-- ================================================= -->
	<!--           responsible department                  -->
	<!-- ================================================= -->
	<sml:contacts>
		<sml:ContactList>
			<!--     manufacturer     -->
			<sml:contact xlink:arcrole="urn:ogc:def:role:OGC:1.0:manufacturer">
				<gmd:CI_ResponsibleParty>
					<gmd:organisationName>
						<gco:CharacterString/>
					</gmd:organisationName>
					<gmd:individualName>
						<gco:CharacterString/>
					</gmd:individualName>
					<!--  contactInfo  -->
					<gmd:contactInfo>
						<gmd:CI_Contact>
							<gmd:phone>
								<gmd:CI_Telephone>
									<gmd:voice/>
								</gmd:CI_Telephone>
							</gmd:phone>
							<gmd:address>
								<gmd:CI_Address>
									<gmd:deliveryPoint/>
									<gmd:city/>
									<gmd:administrativeArea/>
									<gmd:country/>
									<gmd:electronicMailAddress/>
									<gmd:postalCode/>
								</gmd:CI_Address>
							</gmd:address>
						</gmd:CI_Contact>
					</gmd:contactInfo>
				</gmd:CI_ResponsibleParty>
			</sml:contact>
		</sml:ContactList>
	</sml:contacts>
	<!-- ================================================= -->
	<!--       external resource for documentation         -->
	<!-- ================================================= -->
	<sml:documentation>
		<sml:DocumentList>
			<sml:document xlink:arcrole="urn:ogc:def:property:OGC:1.0:OperationsManual">
				<gmd:CI_OnlineResource>
					<gmd:linkage>
						<gmd:URL>null(附件URL)</gmd:URL>
					</gmd:linkage>
					<gmd:name>
						<gco:CharacterString>null(附件名称)</gco:CharacterString>
					</gmd:name>
					<gmd:description>
						<gco:CharacterString>null(附件简单描述)</gco:CharacterString>
					</gmd:description>
				</gmd:CI_OnlineResource>
			</sml:document>
		</sml:DocumentList>
	</sml:documentation>
