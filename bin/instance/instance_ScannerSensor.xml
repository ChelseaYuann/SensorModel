	<!-- ================================================= -->
	<sml:capabilities name="specification">
		<sml:CapabilityList>
			<!--      capabilities==>observationBreath        -->
			<sml:capability name="ScannerRSObservationBreadth">
				<swe:DataRecord definition="urn:ogc:def:property:OGC:1.0:ScannerRSObservationBreadth">
					<swe:label>观测能力广度</swe:label>
					<swe:field name="observedBBox">
						<gml:Envelope definition="urn:ogc:def:property:OGC:1.0:observedBBOX" referenceFrame="urn:ogc:def:crs:OGC:1.3:EPSG:4329">
							<gml:lowerCorner>
								<swe:Vector>
									<swe:coordinate name="纬度">
										<swe:Quantity axisID="y">
											<swe:uom code="deg" xlink:href="urn:ogc:def:uom:OGC:1.0:UCUM::deg"/>
											<swe:value/>
										</swe:Quantity>
									</swe:coordinate>
									<swe:coordinate name="经度">
										<swe:Quantity axisID="x">
											<swe:uom code="deg" xlink:href="urn:ogc:def:uom:OGC:1.0:UCUM::deg"/>
											<swe:value/>
										</swe:Quantity>
									</swe:coordinate>
								</swe:Vector>
							</gml:lowerCorner>
							<gml:upperCorner>
								<swe:Vector>
									<swe:coordinate name="纬度">
										<swe:Quantity axisID="y">
											<swe:uom code="deg" xlink:href="urn:ogc:def:uom:OGC:1.0:UCUM::deg"/>
											<swe:value/>
										</swe:Quantity>
									</swe:coordinate>
									<swe:coordinate name="经度">
										<swe:Quantity axisID="x">
											<swe:uom code="deg" xlink:href="urn:ogc:def:uom:OGC:1.0:UCUM::deg"/>
											<swe:value/>
										</swe:Quantity>
									</swe:coordinate>
								</swe:Vector>
							</gml:upperCorner>
						</gml:Envelope>
					</swe:field>
					<swe:field name="波段类别">
						<swe:Category definition="urn:ogc:def:property:OGC:1.0:bandsCategory">
							<swe:value>VIS/NIR/SWIR/MWIR/TIR等</swe:value>
						</swe:Category>
					</swe:field>
					
					<swe:field name="波段数">
						<swe:Count definition="urn:ogc:def:property:OGC:1.0:bandsNumber">
							<swe:value>00</swe:value>
						</swe:Count>
					</swe:field>
					
					<swe:field name="视场角">
						<swe:Quantity definition="urn:ogc:def:property:OGC:1.0:FOV">
							<swe:uom code="deg"/>
							<swe:value>00</swe:value>
						</swe:Quantity>
					</swe:field>
					
					<swe:field name="侧摆角">
						<swe:Quantity definition="urn:ogc:def:property:OGC:1.0:sideSwingAngle">
							<swe:uom code="deg"/>
							<swe:value>00</swe:value>
						</swe:Quantity>
					</swe:field>
				</swe:DataRecord>
			</sml:capability>
			<!--      capabilities==>observationDepth        -->
			
